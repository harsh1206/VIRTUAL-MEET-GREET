 <link rel="stylesheet" type="text/css" href="/css/room.css">
 
 <div class="chat-container">
  <header class="chat-header">
    <h1><i class="fas fa-smile"></i>ViMeet</h1>
    <button class="btn0" id="start" style="margin-left: 50%;color: white;"  >Start</button>
       <a href="/" class="btn1" style="text-decoration: none;background-color: red;border-radius: 10px;color: white;"  >Leave Room</a>
  </header>
  <main class="chat-main">
    <div class="chat-sidebar">
      <h3 class="heading">Room Name:</h3>
      <h4 id="room-name"></h4>
      <h3 class="heading"> Users</h3>
      <ul id="users">
        
      </ul>
    </div>
    <div class="chat-messages">
        
    </div>
  </main>
  <div class="chat-form-container">
    <form id="chat-form">
      <input
        id="msg"
        type="text"
        placeholder="Enter Message"
        required
        autocomplete="off"
      />
      <button class="btn"><i class="fas fa-paper-plane"></i> Send</button>
    </form>
  </div>
</div>
<div id="question" style=" position: absolute;left: 60%;bottom: 50%;top:20%">
    <h1>IceBreakers</h1> 
    <h2 id="DTU"></h2>
</div>
